{
    "name": "palauaandsons/nova-tags-field",
    "description": "A tags field for Nova apps that uses cartalyst/tags",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-nova"
        }
    ],
    "homepage": "https://github.com/palauaandsons/nova-tags-field",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        },
        {
            "name": "Isern Palaus",
            "email": "isern@palauaandsons.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "cartalyst/tags": ">=8.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "PalauaAndSons\\TagsField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PalauaAndSons\\TagsField\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PalauaAndSons\\TagsField\\TagsFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
